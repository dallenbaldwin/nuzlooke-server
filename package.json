{
   "name": "nuzlooke-server",
   "version": "1.0.0",
   "main": "server.js",
   "scripts": {
      "serve": "npm run dc:up && nodemon --delay 1000ms",
      "start": "NODE_ENV=production node -r dotenv/config server.js",
      "dc:up": "docker-compose up -d ",
      "dc:down": "docker-compose down --remove-orphans",
      "dc:logs": "docker-compose logs -f nuzlooke_localstack"
   },
   "author": "Dallen Baldwin",
   "license": "ISC",
   "dependencies": {
      "@aws-sdk/client-dynamodb": "^3.8.0",
      "aws-sdk": "^2.860.0",
      "axios": "^0.21.1",
      "bcryptjs": "^2.4.3",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "dotenv-expand": "^6.0.1",
      "express": "^4.17.1",
      "google-auth-library": "^7.0.4",
      "jsonwebtoken": "^8.5.1",
      "uuid": "^3.3.2"
   },
   "type": "module"
}